{
  "name": "@passageidentity/passage-elements",
  "version": "1.9.3",
  "private": false,
  "types": "dist/package/index.d.ts",
  "files": [
    "dist/package",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": "^16 || ^18"
  },
  "license": "ISC",
  "exports": {
    ".": {
      "types": "./dist/package/index.d.js"
    },
    "./passage-user": {
      "import": "./dist/package/PassageUser/index.mjs",
      "require": "./dist/package/PassageUser/index.js",
      "types": "./dist/package/PassageUser/index.d.ts"
    },
    "./passage-auth": {
      "import": "./dist/package/passage-auth/index.es.js",
      "require": "./dist/package/passage-auth/index.cjs.js"
    },
    "./passage-login": {
      "import": "./dist/package/passage-login/index.es.js",
      "require": "./dist/package/passage-login/index.cjs.js"
    },
    "./passage-register": {
      "import": "./dist/package/passage-register/index.es.js",
      "require": "./dist/package/passage-register/index.cjs.js"
    },
    "./passage-profile": {
      "import": "./dist/package/passage-profile/index.es.js",
      "require": "./dist/package/passage-profile/index.cjs.js"
    },
    "./passage-passkey-table": {
      "import": "./dist/package/passage-passkey-table/index.es.js",
      "require": "./dist/package/passage-passkey-table/index.cjs.js"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "browser": {
    "./passage-user": "./dist/package/PassageUser/index.js",
    "./passage-auth": "./dist/package/passage-auth/index.cjs.js",
    "./passage-login": "./dist/package/passage-login/index.cjs.js",
    "./passage-register": "./dist/package/passage-register/index.cjs.js",
    "./passage-profile": "./dist/package/passage-profile/index.cjs.js",
    "./passage-passkey-table": "./dist/package/passage-passkey-table/index.cjs.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/package/index.d.ts"
      ],
      "passage-user": [
        "./dist/package/PassageUser/index.d.ts"
      ],
      "passage-auth": [
        "./dist/package/index.d.ts"
      ],
      "passage-login": [
        "./dist/package/index.d.ts"
      ],
      "passage-register": [
        "./dist/package/index.d.ts"
      ],
      "passage-profile": [
        "./dist/package/index.d.ts"
      ],
      "passage-passkey-table": [
        "./dist/package/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "npm run build:package && npm run build:api && npm run build:cdn ",
    "build:api": "tsup ./src/api/PassageUser/index.ts --sourcemap --format esm,cjs --dts-resolve --minify --outDir='dist/package/PassageUser'",
    "build:be_services_local": "APP_ENV=local npm run build",
    "build:be_services_local:watch": "APP_ENV=local vite build --config vite.cdn.config.js --watch",
    "build:cdn": "vite build --config vite.cdn.config.js",
    "build:dev": "APP_ENV=dev npm run build",
    "build:dev:watch": "APP_ENV=dev vite build --config vite.cdn.config.js --watch",
    "build:local": "APP_ENV=local npm run build",
    "build:local:watch": "APP_ENV=local vite build --config vite.cdn.config.js --watch",
    "build:one-passage": "npm run build:dev",
    "build:package": "vite build && npm run build:types",
    "build:prod": "APP_ENV=production npm run build -- --mode=production",
    "build:prod:watch": "APP_ENV=production vite build --config vite.cdn.config.js --watch",
    "build:types": "tsup ./src/index.ts --dts-only --outDir='dist/package'",
    "build:uat": "APP_ENV=uat npm run build",
    "build:uat:watch": "APP_ENV=uat vite build --config vite.cdn.config.js --watch",
    "build:watch": "vite build --config vite.cdn.config.js --watch",
    "check": "vue-tsc --noEmit",
    "clean:dist": "rm -rf dist",
    "clean:node_modules": "rm -rf node_modules",
    "commit:pre": "npx pretty-quick --staged",
    "delete-maps": "npm run delete-map-files && npm run delete-references-to-map-files",
    "delete-map-files": "find ./dist -name '*.map' -delete",
    "delete-references-to-map-files": "npm run delete-references-to-map-files:js && npm run delete-references-to-map-files:mjs",
    "delete-references-to-map-files:js": "find ./dist -name '*.js' -exec sed -i -e 's/sourceMappingURL=[^ ]*.map//g' {} +",
    "delete-references-to-map-files:mjs": "find ./dist -name '*.mjs' -exec sed -i -e 's/sourceMappingURL=[^ ]*.map//g' {} +",
    "deploy:cloud-storage:locales": "gsutil cp -r ./locales gs://passage-frontend",
    "deploy:cloud-storage:locales:uat": "gsutil cp -r ./locales gs://passage-frontend-uat",
    "deploy:cloud-storage:dev": "gsutil -h 'Cache-Control:public, max-age=0' cp ./dist/cdn/index.umd.js gs://cdn.passage.dev/passage-web.js",
    "deploy:cloud-storage:one-passage": "gsutil -h 'Cache-Control:public, max-age=0' cp ./dist/cdn/index.umd.js gs://cdn-1password.passage.dev/passage-web.js",
    "deploy:cloud-storage:prod": "npm run deploy:cloud-storage:prod:passage-elements && npm run deploy:cloud-storage:prod:web",
    "deploy:cloud-storage:uat": "gsutil -h 'Cache-Control:public, max-age=0' cp ./dist/cdn/index.umd.js gs://cdn-uat.passage.dev/passage-web.js",
    "deploy:cloud-storage:prod:passage-elements": "gsutil -h 'Cache-Control:public, max-age=0' cp ./dist/cdn/index.umd.js gs://cdn.passage.id/passage-web.js",
    "deploy:cloud-storage:prod:web": "gsutil -h 'Cache-Control:public, max-age=0' cp ./dist/cdn/index.umd.js gs://psg.so/web.js",
    "format": "pretty-quick --staged",
    "lint": "eslint . --ext .ts,.js,.vue,.json src",
    "lint:fix": "eslint . --ext .ts,.js,.vue,.json src --fix",
    "preview": "vite preview --config vite.cdn.config.js --host --port 4173",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "sentry:release:dev": "env-cmd -f ./.env.dev ./scripts/sentry-release.sh",
    "sentry:release:prod": "env-cmd -f ./.env.production ./scripts/sentry-release.sh",
    "sentry:release:uat": "env-cmd -f ./.env.uat ./scripts/sentry-release.sh",
    "serve:be_services_local": "APP_ENV=local npm run preview",
    "serve:dev": "APP_ENV=dev npm run preview",
    "serve:local": "APP_ENV=local npm run preview",
    "serve:prod": "APP_ENV=production npm run preview",
    "serve:uat": "APP_ENV=uat npm run preview",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@passageidentity/passage-js": "3.6.4",
    "dayjs": "^1.11.0",
    "gridstack": "5.0.0",
    "intl-tel-input": "^17.0.13",
    "jwt-decode": "^3.1.2",
    "ua-parser-js": "^1.0.35",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-inline-svg": "^3.1.0"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@types/intl-tel-input": "^17.0.4",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "2.3.2",
    "@vue/vue3-jest": "^27.0.0",
    "beachball": "^2.31.4",
    "dotenv": "^10.0.0",
    "esbuild": "^0.14.29",
    "eslint-plugin-vue": "^7.14.0",
    "jest": "^27.5.1",
    "sass": "^1.43.2",
    "ts-jest": "^27.1.5",
    "tsup": "^5.11.11",
    "typescript": "^4.3.5",
    "vite": "^2.6.14",
    "vue-tsc": "^0.40.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
