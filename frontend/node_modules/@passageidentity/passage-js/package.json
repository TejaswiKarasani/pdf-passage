{
  "name": "@passageidentity/passage-js",
  "version": "3.6.4",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": "^16 || ^18"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc --noEmit & tsup ./src/index.ts --no-config  --sourcemap --format esm,cjs --dts --minify && npm run build:cdn",
    "build:be_services_local": "npm run build",
    "build:be_services_local:watch": "npm run build:watch",
    "build:cdn": "tsup",
    "build:dev": "npm run build",
    "build:dev:watch": "npm run build:watch",
    "build:local": "npm run build",
    "build:one-passage": "npm run build",
    "build:prod": "npm run build",
    "build:prod:watch": "npm run build:watch",
    "build:uat": "npm run build",
    "build:uat:watch": "npm run build:watch",
    "build:watch": "tsc --noEmit & tsup ./src/index.ts --watch --no-config --format esm,cjs --dts --minify",
    "clean:dist": "rm -rf dist",
    "clean:node_modules": "rm -rf node_modules",
    "commit:pre": "npm run lint && npm run format && git add -A .",
    "config:be_services_local": "echo 'export default {URL: \"http://localhost:3004\", version: \"'$npm_package_name' '$npm_package_version'\"};' > ./src/config/env_config.ts",
    "config:dev": "env-cmd -f ./.env.dev ./scripts/build-config.sh",
    "config:local": "env-cmd -f ./.env.local ./scripts/build-config.sh",
    "config:one-passage": "env-cmd -f ./.env.one-passage ./scripts/build-config.sh",
    "config:prod": "env-cmd -f ./.env.prod ./scripts/build-config.sh",
    "config:uat": "env-cmd -f ./.env.uat ./scripts/build-config.sh",
    "delete-maps": "npm run delete-map-files && npm run delete-references-to-map-files",
    "delete-map-files": "find ./dist ./cdn -name '*.map' -delete",
    "delete-references-to-map-files": "npm run delete-references-to-map-files:js && npm run delete-references-to-map-files:cjs",
    "delete-references-to-map-files:js": "find ./dist ./cdn -name '*.js' -exec sed -i -e 's/ sourceMappingURL=[^ ]*.map//g' {} +",
    "delete-references-to-map-files:cjs": "find ./dist ./cdn -name '*.cjs' -exec sed -i  -e 's/ sourceMappingURL=[^ ]*.map//g' {} +",
    "deploy:cloud-storage:dev": "gsutil -h 'Cache-Control:public, max-age=0' cp ./cdn/index.js gs://cdn.passage.dev/passage-js.js",
    "deploy:cloud-storage:uat": "gsutil -h 'Cache-Control:public, max-age=0' cp ./cdn/index.js gs://cdn-uat.passage.dev/passage-js.js",
    "deploy:cloud-storage:prod": "gsutil -h 'Cache-Control:public, max-age=0' cp ./cdn/index.js gs://cdn.passage.id/passage-js.js",
    "format": "pretty-quick --staged",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "lint:fix": "eslint 'src/**/*.ts?(x)' --fix",
    "sentry:release:dev": "env-cmd -f ./.env.dev ./scripts/sentry-release.sh",
    "sentry:release:prod": "env-cmd -f ./.env.prod ./scripts/sentry-release.sh",
    "sentry:release:uat": "env-cmd -f ./.env.uat ./scripts/sentry-release.sh",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "Passage Identity, Inc.",
  "license": "ISC",
  "devDependencies": {
    "@types/base-64": "^1.0.0",
    "@types/js-cookie": "^3.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "beachball": "^2.31.4",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^12.4.0",
    "ts-jest": "^29.1.0",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@passageidentity/webauthnutils": "1.1.1",
    "@types/node": "^17.0.27",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "serialize-error": "^11.0.0"
  }
}
