{
  "name": "@passageidentity/webauthnutils",
  "version": "1.1.1",
  "description": "A utility library to help with UX decisions around WebAuthn.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": "^16 || ^18"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --noEmit & tsup ./src/index.ts --no-config  --sourcemap --format esm,cjs --dts --minify",
    "build:watch": "tsc --noEmit & tsup ./src/index.ts --watch --no-config --format esm,cjs --dts --minify",
    "serve": "npm run build:watch",
    "commit:pre": "npm run lint && npm run format",
    "format": "pretty-quick --staged",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "lint:fix": "eslint 'src/**/*.ts?(x)' --fix",
    "build:be_services_local": "npm run build",
    "build:dev": "npm run build",
    "build:local": "npm run build",
    "build:one-passage": "npm run build",
    "build:prod": "npm run build",
    "build:uat": "npm run build"
  },
  "author": "Passage Identity, Inc.",
  "license": "ISC",
  "devDependencies": {
    "@types/ua-parser-js": "^0.7.36",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "ua-parser-js": "^1.0.34"
  }
}
